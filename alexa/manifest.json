{
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
  },
  "manifest_version": 2,
  "icons":{
    "128":"icon.png"
  },
  "name": "alexa website rank number badge",
  "description":"showing the current webpage's website rank in real time on the extension button icon",
  "permissions": [
    "activeTab",
    "webNavigation",
    "https://xml.alexa.com/*"
  ],
  "version": "1.1.0",
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["content.js"],
      "css":["style.css"]
    }
  ]
}