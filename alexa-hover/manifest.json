{
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
  },
  "manifest_version": 2,
  "icons":{
    "128":"128x128.png"
  },
  "name": "alexa website rank hover",
  "description":"showing the hyperlink's website rank when mouse hovering on it",
  "permissions": [
    "activeTab",
    "https://www.alexa.com/minisiteinfo/*"
    
  ],
  "version": "1.0.2",
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["content.js"]
    }
  ]
}